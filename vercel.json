{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "https://www.sam2i.fr/:splat",
      "has": [
        {
          "type": "host",
          "value": "!www.sam2i.fr"
        },
        {
          "type": "protocol",
          "value": "http"
        }
      ]
    },
    {
      "source": "/(.*)\\.html",
      "destination": "/:splat"
    },
    {
      "source": "/(.*)\\.xml",
      "destination": "/:splat"
    },
    {
      "source": "/(.*)\\.html",
      "destination": "/:splat.html"
    },
    {
      "source": "/(.*)\\.xml",
      "destination": "/:splat.xml"
    }
  ]
}
