{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "https://www.sam2i.fr/$1",
      "has": [
        {
          "type": "host",
          "value": "!www.sam2i.fr"
        },
        {
          "type": "protocol",
          "value": "http"
        }
      ],
      "status": 301
    },
    {
      "source": "/(.*)\\.html",
      "destination": "/$1",
      "has": [
        {
          "type": "request",
          "key": "User-Agent",
          "value": "Mozilla"
        }
      ],
      "status": 301
    },
    {
      "source": "/(.*)\\.xml",
      "destination": "/$1",
      "has": [
        {
          "type": "request",
          "key": "User-Agent",
          "value": "Mozilla"
        }
      ],
      "status": 301
    },
    {
      "source": "/(.*)\\.html",
      "destination": "/$1.html"
    },
    {
      "source": "/(.*)\\.xml",
      "destination": "/$1.xml"
    }
  ],
  "error_pages": {
    "400": "/erreur",
    "401": "/erreur",
    "402": "/erreur",
    "403": "/erreur",
    "404": "/erreur",
    "500": "/erreur",
    "501": "/erreur",
    "502": "/erreur",
    "503": "/erreur",
    "504": "/erreur"
  }
}